{
  "name": "mammoths-network",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node server/index.js",
    "dev": "NODE_ENV=development node server/index.js",
    "db:init": "node --env-file=.env scripts/db.init.js",
    "db:migrate": "node --env-file=.env scripts/db.migrate.js",
    "jobs:compute-edges": "node --env-file=.env jobs/compute-edges.js",
    "jobs:sync-holders": "node --env-file=.env jobs/sync-holders.js",
    "jobs:sync-activity": "node --env-file=.env jobs/sync-activity.js",
    "jobs:compute-token-metrics": "node --env-file=.env jobs/compute-token-metrics.js",
    "jobs:enrich-wallets": "node --env-file=.env jobs/enrich-wallets.js",
    "jobs:enrich-ethos": "node --env-file=.env jobs/ethos.js",
    "jobs:sync-metadata": "node --env-file=.env jobs/sync-metadata.js",
    "jobs:all": "node --env-file=.env jobs/run-all.js",
    "test:ui": "node scripts/ui.screenshots.js"
  },
  "optionalDependencies": {
    "sharp": "^0.33.4"
  },
  "dependencies": {
    "better-sqlite3": "^9.4.0",
    "compression": "^1.7.4",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "deck.gl": "^8.9.24",
    "@deck.gl/geo-layers": "^8.9.24"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "playwright": "^1.55.0"
  }
